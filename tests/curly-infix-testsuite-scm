; Common Lisp example

(defun factorial (n)
  (if {n <= 1}
       1
       {n * (factorial {n - 1})}))

(defun factorial (n)
  (if (<= n 1)
    1
    (* n (factorial (- n 1)))))

; Scheme example


(define (factorial n)
  (if {n <= 1}
       1
       {n * (factorial {n - 1})}))

(define (factorial  n)
  (if (<= n 1)
    1
    (* n (factorial (- n 1)))))


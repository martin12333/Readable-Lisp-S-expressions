; f 1
; 
;   b c
; 
; before
; ; 
; after
; 
;   separate datums
; 
; let <*
; first
; 
; last
; *>
; 
; ; ((hello) (f x))
; <* hello *>
;   f x
; 
; ; ((bye) a b (f x))
; <* bye *> a b
;   f x
; 
; ; ((one) (two) (three) ((four) (five)) (f x))
; <* one *> <* two *> <* three *>
;   <* four *> <* five *>
;   f x
; 
; . n(expr)
; 
; foo
;   \\ ; comment_eol
;   ; same followed by comment_eol
;   foo
; 
; foo
;   a
;   \\
;   b
; 
; a \\ b
; 
; a
; \\
; b
; 
; foo
; ! c \\ d
; 
; foo
; ! c
; ! \\
; ! d
; 
; f
;  \\ a
;  \\ b
;  \\ c
; 
; \\ a
; \\ b
; \\ c
; 
; foo
; ! bar batz
; ; comment
; ! nitz koo
; 
; car $ gettimeofday()
; 
; '
;   hi
; 
; ' hi
; 
; '
;   one two
; 
; ' one two
; 
; '
;   f()
; 
; ' f()
; 
; '
;   f() g()
; 
; ' f() g()
; 
; '
;   ((f))
; 
; ' ((f))
; 
; ' f(1) g(1)
; 
; ; Multiline - interpret this way so you can access weird extensions
; ; such as multi-line quasiquote ("Bawden quasiquote")
; 
; '
;   f(1)
;   g(1)
; 
; '
;   a
;   b
; 
; ' a b
; 
; (dd1 ee1 . ff1)
; 
; foo1
;  bar
;   aa1
;   bb1
;   .
;   cc1
; 


(define-library
  (srfi 41 primitive)
  (export
    stream-cons
    stream-null
    stream-pair?
    stream-null?
    stream?
    stream-car
    stream-cdr
    stream-lambda))

<* define-library \\ (srfi 41 primitive)
export . (
stream-cons stream-null
stream-pair? stream-null? stream?
stream-car stream-cdr
stream-lambda
)
*>

